{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask: Run on LAN",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "web_app.app",
        "FLASK_ENV": "development",
        "FLASK_RUN_HOST": "0.0.0.0",
        "FLASK_RUN_PORT": "5000"
      },
      "args": ["run"],
      "jinja": true
    }
  ]
}
